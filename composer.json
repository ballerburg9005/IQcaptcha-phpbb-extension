{
    "name": "ballerburg9005/iqcaptcha",
    "type": "phpbb-extension",
    "description": "IQcaptcha tests against IQ with Raven's progressive matrices and high school calculus. Works just like Google's reCAPTCHA.",
    "homepage": "https://github.com/ballerburg9005/IQcaptcha-phpbb",
    "version": "0.1",
    "time": "2021-04-01",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ballerburg9005",
            "homepage": "http://iqcaptcha.us.to",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.6,<3.4@dev"

    },
    "extra": {
        "display-name": "IQcaptcha",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.6,<3.4@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/iqcaptcha",
            "filename": "version_check"
        }
    }
}
